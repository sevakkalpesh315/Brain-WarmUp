<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sevakkalpesh.BrainWarmup"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/brainwarmup"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sevakkalpesh.BrainWarmup.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sevakkalpesh.BrainWarmup.MainActivity" >
        </activity>
        <activity
            android:name="com.sevakkalpesh.BrainWarmup.PlayGame"
            android:configChanges="keyboardHidden|orientation|screenSize" >
        </activity>
        <activity android:name="com.sevakkalpesh.BrainWarmup.HowToPlay" >
        </activity>
        <activity android:name="com.sevakkalpesh.BrainWarmup.HighScores" >
        </activity>
    </application>

</manifest>